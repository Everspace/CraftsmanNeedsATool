{
  "compilerOptions": {
    "target": "es5",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "lib": [
      "es2017",
      "dom"
    ],
    "resolveJsonModule": true,
    "noEmit": true,
    "jsx": "preserve",
    "isolatedModules": true,
    "checkJs": false,
    "noImplicitAny": false,
    "typeRoots": [
      "@types"
    ],
    "sourceMap": true
  },
  "exclude": [
    "src/setupProxy.js",
    "*.test.*"
  ],
  "include": [
    "src"
  ]
}
